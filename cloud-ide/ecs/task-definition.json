{
  "family": "cloudide-worker",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::170722810509:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::170722810509:role/cloudide-worker-task-role",
  "containerDefinitions": [
    {
      "name": "cloudide-worker",
      "image": "170722810509.dkr.ecr.ap-south-1.amazonaws.com/cloudide-worker:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cloudide-worker",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "AWS_REGION",
          "value": "ap-south-1"
        },
        {
          "name": "MONGODB_URI",
          "value": "mongodb://localhost:27017/cloudide"
        },
        {
          "name": "REDIS_HOST",
          "value": "localhost"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": "AKIASPP67LKGXXX3V47C"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "hHXvr9FweFygeTSVTUJoamaAZI/XTvnHYZH6gdBn"
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "value": "cloudide-projects"
        },
        {
          "name": "AWS_SQS_QUEUE_URL",
          "value": "https://sqs.ap-south-1.amazonaws.com/170722810509/cloudide-terminal-queue"
        }
      ]
    }
  ]
}
