{
  "name": "cloud-ide",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && tsc",
    "start": "next start",
    "lint": "eslint",
    "ws-server": "tsx server.ts",
    "worker": "tsx workers/terminal-worker.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run ws-server\"",
    "dev:worker": "tsx workers/terminal-worker.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run ws-server\" \"npm run dev:worker\"",
    "ecs:setup": "chmod +x scripts/setup-ecs.sh && ./scripts/setup-ecs.sh",
    "ecs:orchestrator": "tsx workers/ecs-orchestrator.ts",
    "ecs:test": "tsx workers/terminal-worker.ts",
    "ecs:logs": "aws logs tail /ecs/cloudide-worker --follow",
    "ecs:tasks": "aws ecs list-tasks --cluster cloudide-cluster --desired-status RUNNING",
    "ecs:build-push": "aws ecr get-login-password --region ${AWS_REGION:-ap-south-1} | docker login --username AWS --password-stdin $(aws sts get-caller-identity --query Account --output text).dkr.ecr.${AWS_REGION:-ap-south-1}.amazonaws.com && docker build -f Dockerfile.worker -t cloudide-worker:latest . && docker tag cloudide-worker:latest $(aws sts get-caller-identity --query Account --output text).dkr.ecr.${AWS_REGION:-ap-south-1}.amazonaws.com/cloudide-worker:latest && docker push $(aws sts get-caller-identity --query Account --output text).dkr.ecr.${AWS_REGION:-ap-south-1}.amazonaws.com/cloudide-worker:latest"
  },
